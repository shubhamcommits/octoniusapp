{
  "name": "octonius",
  "description": "Desktop application of Octonius",
  "author": {
    "name": "Team Octonius",
    "email": "dev@octonius.com"
  },
  "version": "1.0.0",
  "build": {
    "directories": {
      "output": "release/"
    }
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:en": "ng serve --configuration=en",
    "start:de": "ng serve --configuration=de",
    "start:es": "ng serve --configuration=es",
    "start:fa": "ng serve --configuration=fa",
    "start:hu": "ng serve --configuration=hu",
    "build:en": "ng build --aot --configuration=en",
    "build:de": "ng build --aot --configuration=de",
    "build:es": "ng build --aot --configuration=es",
    "build:fa": "ng build --aot --configuration=fa",
    "build:hu": "ng build --aot --configuration=hu",
    "dev": "ng serve",
    "build": "ng build --aot",
    "prod": "ng build --aot --configuration production --localize",
    "gzip": "gzipper --verbose ./dist ./gzipped",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "i18n:extract": "ng xi18n --output-path src/locale"
  },
  "private": true,
