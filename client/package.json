  "scripts": {
    "ng": "ng",
    "start": "ng serve ",
    "dev": "ng serve ",
    "build": "ng build",
    "prod": "ng build --prod",
    "gzip": "gzipper --verbose ./dist ./gzipped",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "app:linux": "ng build --prod --base-href ./ && electron-builder build --linux",
    "app:mac": "ng build --prod --base-href ./ && electron-builder build --mac",
    "app:windows": "ng build --prod --base-href ./ && electron-builder build --windows",
    "electron": "ng build --prod --base-href ./ && electron .",
    "postinstall": "ngcc"
  },
