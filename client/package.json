{
  "name": "octonius",
  "description": "Desktop application of Octonius",
  "author": {
    "name": "Team Octonius",
    "email": "dev@octonius.com"
  },
  "version": "1.0.0",
  "main": "main.js",
  "build": {
    "directories": {
      "output": "release/"
    }
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "ng serve",
    "build": "ng build --localize",
    "prod": "ng build --configuration --localize production",
    "gzip": "gzipper --verbose ./dist ./gzipped",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "app:linux": "ng build --configuration --localize production --base-href ./ && electron-builder build --linux",
    "app:mac": "ng build --configuration --localize production --base-href ./ && electron-builder build --mac",
    "app:windows": "ng build --configuration --localize production --base-href ./ && electron-builder build --windows",
    "electron": "ng build --configuration --localize production --base-href ./ && electron .",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.1.3",
    "@angular/cdk": "12.1.3",
    "@angular/common": "^12.1.3",
