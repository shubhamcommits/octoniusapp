{
	"extends": "airbnb",
	"parserOptions": {
		"ecmaVersion": 6,
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"node": true,
		"browser": true,
		"es6": true,
		"jest": true
	},
	"rules": {
		"comma-dangle": [
			"error",
			"never"
		],
		"no-unused-vars": [
			"error",
			{
				"vars": "local",
				"args": "none"
			}
		]
	}
}
