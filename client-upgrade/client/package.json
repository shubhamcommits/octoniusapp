{
  "name": "client",
  "description": "Client Side application of Octonius",
  "author": {
    "name": "Team Octonius",
    "email": "dev@octonius.com"
  },
  "version": "0.0.0",
  "main": "main.js",
  "build": {
    "directories": {
      "output": "release/"
    }
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "prod": "ng build --prod",
    "gzip": "gzipper --verbose ./dist ./gzipped",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron-build:linux": "ng build --prod --base-href ./ && electron-builder build --linux",
    "electron-build:mac": "ng build --prod --base-href ./ && electron-builder build --mac",
    "electron-build:windows": "ng build --prod --base-href ./ && electron-builder build --windows",
    "electron": "ng build --prod --base-href ./ && electron ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
